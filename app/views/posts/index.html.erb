<p>posts is saved</p>

<% @posts.each do |p| %>
  <%= p.choice %><br>
  <%= simple_format(p.body) %><br>
  <%= p.address %><br>
  <%= p.created_at %><br>
  <%= User.find(p.user_id).username %>
  <div>
  <%= image_tag "http://maps.googleapis.com/maps/api/staticmap?center=#{p.latitude},#{p.longitude}&markers=#{p.latitude},#{p.longitude}&zoom=17&size=640x400&key=AIzaSyBOt93SZcyo4Yygy6JcwscWRNsbZzt9X1Q",
    class: 'img-fluid img-rounded', alt: "#{p.address} on the map"%>
    </div>
  <% end %>
  